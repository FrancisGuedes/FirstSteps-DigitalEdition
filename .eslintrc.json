{
    "env": {
        "browser": true
        
    },
    "extends": [
        "react-app", 
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier" 
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "printWidth": 80,
                
                
                
                "useTabs": true
            }
        ]
    }
}
